@page
@using Microsoft.AspNetCore.Mvc.Localization
@using JS.Abp.DataDictionary.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using JS.Abp.DataDictionary.Web.Pages.DataDictionary.DataDictionaries
@using JS.Abp.DataDictionary.DataDictionaries;
@using System.Globalization
@inject IHtmlLocalizer<DataDictionaryResource> L
@model CreateModalModel
@{
    Layout = null;
}


<form data-ajaxForm="true" asp-page="/DataDictionary/DataDictionaries/CreateModal" autocomplete="off">
    <abp-modal id="DataDictionaryCreateModal">
        <abp-modal-header title="@L["NewDataDictionary"].Value"></abp-modal-header>

        <abp-modal-body>

                    

            <abp-input asp-for="DataDictionary.Code"   />


            <abp-input asp-for="DataDictionary.DisplayText"   />


            <abp-input asp-for="DataDictionary.Description" text-area  />


            <abp-input asp-for="DataDictionary.IsActive"   />

                    
                


        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)">

        </abp-modal-footer>
    </abp-modal>
</form>
